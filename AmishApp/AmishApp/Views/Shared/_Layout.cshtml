<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <meta name="description" content="Admin, Dashboard, Bootstrap" />
    <link rel="shortcut icon" sizes="196x196" href="../assets/images/logo.png">
    <title>AmIsH - APP</title>

    <link rel="stylesheet" href="~/lib/bower/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/lib/bower/material-design-iconic-font/dist/css/material-design-iconic-font.css">
    <!-- build:css ../assets/css/app.min.css -->
    <link rel="stylesheet" href="~/lib/bower/animate.css/animate.min.css">
    <link rel="stylesheet" href="~/lib/bower/fullcalendar/dist/fullcalendar.min.css">
    <link rel="stylesheet" href="~/lib/bower/perfect-scrollbar/css/perfect-scrollbar.css">
    <link rel="stylesheet" href="~/css/bootstrap.css">
    <link rel="stylesheet" href="~/css/core.css">
    <link rel="stylesheet" href="~/css/app.css">
    <!-- endbuild -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Raleway:400,500,600,700,800,900,300">
    <script src="../lib/bower/breakpoints.js/dist/breakpoints.min.js"></script>
    <script>
        Breakpoints();
    </script>
</head>
<body class="menubar-left menubar-unfold menubar-light theme-primary">
    <!-- APP NAVBAR ==========-->
    <nav id="app-navbar" class="navbar navbar-inverse navbar-fixed-top primary">

        <!-- navbar header -->
        <div class="navbar-header">
            <button type="button" class="navbar-toggle navbar-toggle-right collapsed" data-toggle="collapse" data-target="#app-navbar-collapse" aria-expanded="false">
                <span class="sr-only">Toggle navigation</span>
                <span class="zmdi zmdi-hc-lg zmdi-more"></span>
            </button>

            <a href="index.html" class="navbar-brand">
                <span class="brand-icon"><i class="fa fa-gg"></i></span>
                <span class="brand-name">AmIsH App</span>
            </a>
        </div><!-- .navbar-header -->

        <div class="navbar-container container-fluid">
            <div class="collapse navbar-collapse" id="app-navbar-collapse">
                <ul class="nav navbar-toolbar navbar-toolbar-left navbar-left">
                    <li class="hidden-float hidden-menubar-top">
                        <a href="javascript:void(0)" role="button" id="menubar-fold-btn" class="hamburger hamburger--arrowalt is-active js-hamburger">
                            <span class="hamburger-box"><span class="hamburger-inner"></span></span>
                        </a>
                    </li>
                    <li>
                        <h5 class="page-title hidden-menubar-top hidden-float">AmIsH App</h5>
                    </li>
                </ul>
            </div>
        </div><!-- navbar-container -->
    </nav>
    <!--========== END app navbar -->
    <!-- APP ASIDE ==========-->
    <aside id="menubar" class="menubar light">
        <div class="app-user">
            <div class="media">
                <div class="media-left">
                    <div class="avatar avatar-md avatar-circle">
                        <a href="javascript:void(0)"><img class="img-responsive" src="/assets/images/original/221.jpg" alt="avatar" /></a>
                    </div><!-- .avatar -->
                </div>
                <div class="media-body">
                    <div class="foldable">
                        <h5><a href="javascript:void(0)" class="username">Comercio - Las Pantuflas</a></h5>
                        <ul>
                            <li class="dropdown">
                                <a href="javascript:void(0)" class="dropdown-toggle usertitle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <small>Vendedor</small>
                                    <span class="caret"></span>
                                </a>
                                <ul class="dropdown-menu animated flipInY">
                                    <li>
                                        <a class="text-color" href="/index.html">
                                            <span class="m-r-xs"><i class="fa fa-search"></i></span>
                                            <span>Buscar Guia</span>
                                        </a>
                                    </li>
                                    <li>
                                        <a class="text-color" href="profile.html">
                                            <span class="m-r-xs"><i class="fa fa-truck"></i></span>
                                            <span>Solicitar Operador</span>
                                        </a>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                </div><!-- .media-body -->
            </div><!-- .media -->
        </div><!-- .app-user -->
    </aside>
    <!--========== END app aside -->
    <div class="container">
        <main role="main" id="app-main" class="app-main">
            <div class="wrap">
                @RenderBody()
            </div>
        </main>
    </div>

    <div class="wrap p-t-0">
        <footer class="app-footer">
            <div class="clearfix">
                <div class="copyright pull-left">Copyright AmIsH 2020 &copy;</div>
            </div>
        </footer>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <!-- build:js ../assets/js/core.min.js -->
    <script src="~/lib/bower/jquery/dist/jquery.js"></script>
    <script src="~/lib/bower/jquery-ui/jquery-ui.min.js"></script>
    <script src="~/lib/bower/jQuery-Storage-API/jquery.storageapi.min.js"></script>
    <script src="~/lib/bower/bootstrap-sass/assets/javascripts/bootstrap.js"></script>
    <script src="~/lib/bower/jquery-slimscroll/jquery.slimscroll.js"></script>
    <script src="~/lib/bower/perfect-scrollbar/js/perfect-scrollbar.jquery.js"></script>
    <script src="~/lib/bower/PACE/pace.min.js"></script>
    <!-- endbuild -->
    <!-- build:js ../assets/js/app.min.js -->
    <script src="~/js/library.js"></script>
    <script src="~/js/plugins.js"></script>
    <script src="~/js/app.js"></script>
    <!-- endbuild -->
    @RenderSection("Scripts", required: false)
</body>
</html>
